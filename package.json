{
  "name": "rbs-snippets",
  "displayName": "RBS Snippets",
  "description": "RBS snippets for VSCode",
  "publisher": "MateuszDrewniak",
  "version": "1.1.2",
  "icon": "images/logo.jpeg",
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:ruby",
    "onLanguage:rbs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Verseth/vscode-rbs-snippets"
  },
  "galleryBanner": {
    "color": "#804c0c",
    "theme": "dark"
  },
  "license": "MIT",
  "keywords": [
    "rbs",
    "ruby",
    "ruby rbs",
    "snippet",
    "snippets",
    "ruby types"
  ],
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "contributes": {
    "snippets": [
      {
        "language": "rbs",
        "path": "./snippets/rbs.code-snippets"
      },
      {
        "language": "ruby",
        "path": "./snippets/rbs-ruby.code-snippets"
      }
    ],
    "commands": [
      {
        "command": "rbs-snippets.convertSigsToRBSInFile",
        "title": "RBS: convert sigs to RBS in the file"
      }
    ]
  },
  "devDependencies": {
    "@types/vscode": "^1.98.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^9.21.0",
    "typescript": "^5.7.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
